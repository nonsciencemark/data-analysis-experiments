}

@article{Dvorak2014,
  title = {Synechococcus: 3 Billion Years of Global Dominance},
  shorttitle = {Synechococcus},
  author = {Dvo{\v r}{\'a}k, Petr and Casamatta, Dale A. and Poul{\'i}{\v c}kov{\'a}, Aloisie and Ha{\v s}ler, Petr and Ond{\v r}ej, Vladan and Sanges, Remo},
