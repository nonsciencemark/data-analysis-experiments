@article{Jarillo2022,
  title = {Spatial and {{Ecological Scaling}} of {{Stability}} in {{Spatial Community Networks}}},
  author = {Jarillo, Javier and {Cao-Garc{\'i}a}, Francisco J. and De Laender, Frederik},
