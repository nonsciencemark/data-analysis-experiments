@article{Ritz2010,
  title = {Toward a Unified Approach to Dose\textendash Response Modeling in Ecotoxicology},
  author = {Ritz, Christian},
  year = {2010},
  journal = {Environmental Toxicology and Chemistry},
  volume = {29},
