@article{Gollner2017,
  title = {Resilience of Benthic Deep-Sea Fauna to Mining Activities},
  author = {Gollner, Sabine and Kaiser, Stefanie and Menzel, Lena and Jones, Daniel O.B. and Brown, Alastair and Mestre, Nelia C. and {van Oevelen}, Dick and Menot, Lenaick and Cola{\c c}o, Ana and Canals, Miquel and Cuvelier, Daphne and Durden, Jennifer M. and Gebruk, Andrey and Egho, Great A. and Haeckel, Matthias and Marcon, Yann and Mevenkamp, Lisa and Morato, Telmo and Pham, Christopher K. and Purser, Autun and {Sanchez-Vidal}, Anna and Vanreusel, Ann and Vink, Annemiek and Martinez Arbizu, Pedro},
  year = {2017},
  journal = {Marine Environmental Research},
  volume = {129},
  pages = {76--101},
  publisher = {{Elsevier Ltd}},
  issn = {18790291},
  doi = {10.1016/j.marenvres.2017.04.010},
  abstract = {With increasing demand for mineral resources, extraction of polymetallic sulphides at hydrothermal vents, cobalt-rich ferromanganese crusts at seamounts, and polymetallic nodules on abyssal plains may be imminent. Here, we shortly introduce ecosystem characteristics of mining areas, report on recent mining developments, and identify potential stress and disturbances created by mining. We analyze species' potential resistance to future mining and perform meta-analyses on population density and diversity recovery after disturbances most similar to mining: volcanic eruptions at vents, fisheries on seamounts, and experiments that mimic nodule mining on abyssal plains. We report wide variation in recovery rates among taxa, size, and mobility of fauna. While densities and diversities of some taxa can recover to or even exceed pre-disturbance levels, community composition remains affected after decades. The loss of hard substrata or alteration of substrata composition may cause substantial community shifts that persist over geological timescales at mined sites.},
  pmid = {28487161},
  keywords = {Abyssal plains,Benthos,Deep ocean,Disturbance,Hydrothermal vents,Minerals,Mining,Recovery,Resilience,Seamounts}
}

