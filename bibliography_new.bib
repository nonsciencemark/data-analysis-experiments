@article{VanMoorsel2019,
  title = {Evidence for Rapid Evolution in a Grassland Biodiversity Experiment},
  author = {{van Moorsel}, Sofia J. and Schmid, Marc W. and Wagemaker, Niels C. A. M. and {van Gurp}, Thomas and Schmid, Bernhard and Vergeer, Philippine},
