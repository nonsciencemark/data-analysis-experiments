@article{Binzer2016,
  title = {Interactive Effects of Warming, Eutrophication and Size Structure: {{Impacts}} on Biodiversity and Food-Web Structure},
  author = {Binzer, Amrei and Guill, Christian and Rall, Bj{\"o}rn C. and Brose, Ulrich},
