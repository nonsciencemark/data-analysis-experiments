@article{Olofsson2009,
  title = {Bet-Hedging as an Evolutionary Game: {{The}} Trade-off between Egg Size and Number},
  shorttitle = {Bet-Hedging as an Evolutionary Game},
  author = {Olofsson, Helen and Ripa, J{\"o}rgen and Jonz{\'e}n, Niclas},
  year = {2009},
