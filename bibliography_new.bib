@article{Scalco2014,
  title = {The Dynamics of Sexual Phase in the Marine Diatom {{Pseudo-nitzschia}} Multistriata ({{Bacillariophyceae}})},
  author = {Scalco, Eleonora and Stec, Krzysztof and Iudicone, Daniele and Ferrante, Maria Immacolata and Montresor, Marina},
  year = {2014},
  journal = {Journal of Phycology},
  volume = {50},
