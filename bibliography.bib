  author = {Tsiola, Anastasia and Pitta, Paraskevi and Giannakourou, Antonia and Bourdin, Guillaume and Marro, Sophie and Maugendre, Laure and Pedrotti, Maria Luiza and Gazeau, Fr{\'e}d{\'e}ric},
