@article{VanDenBrink2016,
  title = {New Approaches to the Ecological Risk Assessment of Multiple Stressors},
  author = {Van Den Brink, Paul J. and Choung, Catherine Bo and Landis, Wayne and {Mayer-Pinto}, Mariana and Pettigrove, Vincent and Scanes, Peter and Smith, Rachael and Stauber, Jenny},
  year = {2016},
  journal = {Marine and Freshwater Research},
  volume = {67},
