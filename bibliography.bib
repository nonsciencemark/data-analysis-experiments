@article{Gomez-Mestre2013,
  title = {A Heuristic Model on the Role of Plasticity in Adaptive Evolution: {{Plasticity}} Increases Adaptation, Population Viability and Genetic Variation},
  author = {{Gomez-Mestre}, Ivan and Jovani, Roger},
  year = {2013},
  journal = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {280},
  number = {1771},
  issn = {14712954},
  doi = {10.1098/rspb.2013.1869},
  abstract = {An ongoing new synthesis in evolutionary theory is expanding our view of the sources of heritable variation beyond point mutations of fixed phenotypic effects to include environmentally sensitive changes in gene regulation. This expansion of the paradigm is necessary given ample evidence for a heritable ability to alter gene expression in response to environmental cues. In consequence, single genotypes are often capable of adaptively expressing different phenotypes in different environments, i.e. are adaptively plastic. We present an individual-based heuristic model to compare the adaptive dynamics of populations composed of plastic or non-plastic genotypes under a wide range of scenarios where we modify environmental variation, mutation rate and costs of plasticity. The model shows that adaptive plasticity contributes to the maintenance of genetic variation within populations, reduces bottlenecks when facing rapid environmental changes and confers an overall faster rate of adaptation. In fluctuating environments, plasticity is favoured by selection and maintained in the population. However, if the environment stabilizes and costs of plasticity are high, plasticity is reduced by selection, leading to genetic assimilation, which could result in species diversification. More broadly, our model shows that adaptive plasticity is a common consequence of selection under environmental heterogeneity, and hence a potentially common phenomenon in nature. Thus, taking adaptive plasticity into account substantially extends our view of adaptive evolution. \textcopyright{} 2013 The Author(s) Published by the Royal Society. All rights reserved.},
  keywords = {Adaptation,Bottleneck,Developmental plasticity,Genetic accommodation,Genetic variation,Heterogeneous environment},
  file = {C:\Users\mholmes\Zotero\storage\ZHZFL65C\Gomez-Mestre_Jovani_2013_A heuristic model on the role of plasticity in adaptive evolution.pdf}
}

@article{GoncalvesLeles2018,
  title = {Modelling Mixotrophic Functional Diversity and Implications for Ecosystem Function},
  author = {Gon{\c c}alves Leles, Suzana and Polimene, Luca and Bruggeman, Jorn and Blackford, Jeremy and Ciavatta, Stefano and Mitra, Aditee and Flynn, Kevin John},
