}

@article{Paavola2005,
  title = {Are Invasive Species Most Successful in Habitats of Low Native Species Richness across {{European}} Brackish Water Seas?},
  author = {Paavola, Marjo and Olenin, Sergej and Lepp{\"a}koski, Erkki},
