}

@article{Xiao2013,
  title = {Armstrong-{{McGehee}} Mechanism Revisited: {{Competitive}} Exclusion and Coexistence of Nonlinear Consumers},
  author = {Xiao, Xiao and Fussmann, Gregor F.},
  year = {2013},
  journal = {Journal of Theoretical Biology},
  volume = {339},
