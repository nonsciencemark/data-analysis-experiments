@article{VanWichelen2013,
  title = {Planktonic Ciliate Community Structure in Shallow Lakes of Lowland {{Western Europe}}},
  author = {Van Wichelen, Jeroen and Johansson, Liselotte S. and Vanormelingen, Pieter and Declerck, Steven A. J. and Lauridsen, Torben L. and De Meester, Luc and Jeppesen, Erik and Vyverman, Wim},
