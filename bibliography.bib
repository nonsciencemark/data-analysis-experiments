@article{Wohlgemuth2017,
  title = {Prey Diversity Effects on Ecosystem Functioning Depend on Consumer Identity and Prey Composition},
  author = {Wohlgemuth, Daniel and Filip, Joanna and Hillebrand, Helmut and Moorthi, Stefanie D.},
