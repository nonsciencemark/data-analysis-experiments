@article{Wang2014,
  title = {Ecosystem Stability in Space: {{$\alpha$}}, {{$\beta$}} and {{$\gamma$}} Variability},
  author = {Wang, Shaopeng and Loreau, Michel},
  year = {2014},
  journal = {Ecology Letters},
