@article{Uszko2017a,
  title = {Effects of Warming on Predator-Prey Interactions - a Resource-Based Approach and a Theoretical Synthesis},
  author = {Uszko, Wojciech and Diehl, Sebastian and Englund, G{\"o}ran and Amarasekare, Priyanga},
