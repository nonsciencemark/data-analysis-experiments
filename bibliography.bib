@article{Brussaard1996,
  title = {Impact of Phytoplankton Bloom Magnitude on a Pelagic Microbial Food Web},
  author = {Brussaard, C P D and Gast, G. J. and Van Duyl, F. C. and Riegman, R.},
  year = {1996},
  journal = {Marine Ecology Progress Series},
  volume = {144},
  number = {1-3},
  pages = {211--221},
  issn = {01718630},
  doi = {10.3354/meps144211},
  abstract = {The impact of phytoplankton bloom magnitude on the microbial food web of the Dutch pelagic coastal zone (Southern Bight of the North Sea) was studied during spring 1993. Data were compared to a study carried out in 1992 at the same location. Light availability and temperature were the key factors for the timing of the diatom spring bloom. Specific algal cell lysis rates during the diatom bloom were always \textexclamdown 0.1/d. Due to sinking of the diatoms no effect of the difference in bloom magnitude between the 2 years on the pelagic microbial food web was detected. A high onset level of nitrate (55 mu M) resulted in a dense bloom of the colonial Phaeocystis, with maximum cell numbers of 1.8 x 10 super(8) cells /l. During the development of the bloom, the pH of the seawater increased from 7.9 to 8.7. The bloom of Phaeocystis declined through cell lysis (up to 0.3/d). Due to the enhanced biomass of Phaeocystis the amounts of released algal cellular carbon were high (up to 400 mu g C/l/d). Differences in Phaeocystis bloom magnitude were, as a consequence of algal cell lysis, reflected in bacterial production (up to 90 mu g C/l/d in 1993) and grazing activity of members of the microbial food web. The elevated importance of the microbial food web resulted in a high standing stock of microzooplankton (850 mu g C/l) and accumulation of regenerated inorganic nutrients. Calculations on both carbon and nitrogen budgets indicate that most of the Phaeocystis bloom was mineralized within the water column.},
  isbn = {0171-8630},
  keywords = {Bacterial production,Cell lysis,Diatoms,Microbial food web,Mineralization,Phaeocystis,Phytoplankton bloom magnitude}
}

@article{Brussaard1997,
  title = {Biodiversity and Ecosystem Functioning in Soil},
  author = {Brussaard, Lijbert and {Behan-Pelletier}, Val M. and Bignell, David E. and Brown, Valerie K. and Didden, Wim and Folgarait, Patricia and Fragoso, Carlos and Freckman, Diana Wall and Gupta, Vadakattu V.S.R. and Hattori'S, Tsutomu and Hawksworth, David L. and Klopatek, Carole and Lavelle, Patrick and Malloch, David W. and Rusek, Josef and S{\"o}derstr{\"o}m, Bengt and Tiedje, James M. and Virginia, Ross A.},
  year = {1997},
  journal = {Ambio},
  volume = {26},
  number = {8},
  pages = {563--570},
  issn = {00447447},
  doi = {10.1146/annurev-ecolsys-120213-091917},
  abstract = {We review the current knowledge on biodiversity in soils, its role in ecosystem processes, its importance for human purposes, and its resilience against stress and disturbance. The number of existing species is vastly higher than the number described, even in the macroscopically visible taxa, and biogeographical syntheses are largely lacking. A major effort in taxonomy and the training of a new generation of systematists is imperative. This effort has to e focussed on the groups of soil organisms that, to the best of our knowledge, play key roles in ecosystem functioning. To identify such groups, spheres of influence (SO1) of soil biota - such as the root biota, the shredders of organic matter and the soil bioturbators - are recognized that presumably control ecosystem processes, for example, through interactions with plants. Within those SO1, functional groups of soil organisms are recognized. Research questions of the highest urgency are the assignment of species to functional groups and determining the redundancy of species within functional groups. These priorities follow from the need to address the extent of any loss of functioning in soils, associated with intensive agriculture, forest disturbance, pollution of the environment, and global environmental change. The soil biota considered at present to be most at risk are species-poor functional groups among macrofaunal shredders of organic matter, bioturbators of soil, specialized bacteria like nitrifiers and nitrogen fixers, and fungiforming mycorrhizas. An experimental approach in addressing these research priorities is needed, using long-term and large-scale field experiments and modern methods of geostatistics and geographic information systems.},
  keywords = {biodiversity,coexistence,complementarity,ecosystem functioning,invasibility,invasibility complementarity,niche differentiation,productivity,stability},
  file = {C\:\\Users\\mholmes\\Zotero\\storage\\4A2IHFF8\\Brussaard et al_1997_Biodiversity and ecosystem functioning in soil.pdf;C\:\\Users\\mholmes\\Zotero\\storage\\HSU4GWRW\\Tilman et al. - 2014 - Biodiversity and Ecosystem Functioning.pdf}
}

