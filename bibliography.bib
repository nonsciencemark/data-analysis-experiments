@article{Gaston2008,
  title = {The {{Ecological Performance}} of {{Protected Areas}}},
  author = {Gaston, Kevin J. and Jackson, Sarah F. and {Cant{\'u}-Salazar}, Lisette and {Cruz-Pi{\~n}{\'o}n}, Gabriela},
