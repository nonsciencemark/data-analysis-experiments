@article{Negri2011,
  title = {Herbicides Increase the Vulnerability of Corals to Rising Sea Surface Temperature},
  author = {Negri, Andrew P. and Flores, Florita and R{\"o}thig, Till and Uthicke, Sven},
  year = {2011},
  journal = {Limnology and Oceanography},
  volume = {56},
