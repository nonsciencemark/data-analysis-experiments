@article{Gu2020,
  title = {Effect of {{Warming}} on {{Growth}}, {{Grazing}}, and {{Community Composition}} of {{Free-Living Bacterioplankton}} in {{Subtropical Coastal Waters During Winter}} and {{Summer}}},
