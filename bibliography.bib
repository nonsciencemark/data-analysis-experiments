@article{Lee2016,
  title = {Lethal and Sub-Lethal Effects of Elevated {{CO2}} Concentrations on Marine Benthic Invertebrates and Fish},
  author = {Lee, C and Hong, S and Kwon, B O and Lee, J H and Ryu, J and Park, Y G and Kang, S G and Khim, J S},
  year = {2016},
  journal = {Environmental Science and Pollution Research},
  volume = {23},
