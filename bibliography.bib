@article{Six2007,
  title = {Diversity and Evolution of Phycobilisomes in Marine {{Synechococcus}} Spp.: {{A}} Comparative Genomics Study},
  author = {Six, Christophe and Thomas, Jean Claude and Garczarek, Laurence and Ostrowski, Martin and Dufresne, Alexis and Blot, Nicolas and Scanlan, David J. and Partensky, Fr{\'e}d{\'e}ric},
