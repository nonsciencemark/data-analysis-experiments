  author = {Mainali, Kumar P. and Warren, Dan L. and Dhileepan, Kunjithapatham and Mcconnachie, Andrew and Strathie, Lorraine and Hassan, Gul and Karki, Debendra and Shrestha, Bharat B. and Parmesan, Camille},
  year = {2015},
