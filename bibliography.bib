  keywords = {Bacterial systems biology,Ecological modelling,Microbial ecology,Population dynamics},
  file = {C:\Users\mholmes\Zotero\storage\3ECM4FUJ\Abreu et al_2019_Mortality causes universal changes in microbial community composition.pdf}
