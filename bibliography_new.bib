}

@article{Clark2015a,
  title = {Spatial Convergent Cross Mapping to Detect Causal Relationships from Short Time Series},
  author = {Clark, Adam Thomas and Ye, Hao and Isbell, Forest and Deyle, Ethan R. and Cowles, Jane and Tilman, G. David and Sugihara, George},
