@article{DeLorenzo2003,
  title = {Individual and Mixture Toxicity of {{Three}} Pesticides; Atrazine, Chlorpyrifos, and Chlorothalonil to the Marine Phytoplankton Species {{Dunaliella}} Tertiolecta},
  author = {DeLorenzo, Marie E. and Serrano, Lorimar},
