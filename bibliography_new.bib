@article{Wieczynski2021,
  title = {Linking Species Traits and Demography to Explain Complex Temperature Responses across Levels of Organization},
  author = {Wieczynski, Daniel J. and Singla, Pranav and Doan, Adrian and Singleton, Alexandra and Han, Ze-Yi and Votzke, Samantha and Yammine, Andrea and Gibert, Jean P.},
