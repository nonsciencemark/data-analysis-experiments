  author = {Loreau, M. and Naeem, S. and Inchausti, P. and Bengtsson, J. and Grime, J. P. and Hector, A. and Hooper, D. U. and Huston, M. A. and Raffaelli, D. and Schmid, B. and Tilman, D. and Wardle, D. A.},
