@incollection{Frankel2013,
  title = {Cell Biology of Tetrahymena Thermophila},
  booktitle = {Methods in Cell Biology},
  author = {Frankel, Joseph},
  editor = {Asai, David J. and Forney, James D.},
  year = {2013},
  volume = {62},
