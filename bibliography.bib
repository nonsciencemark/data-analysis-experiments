@article{Lindstrom2003,
  title = {Live Sorting and Survival of Unstained and {{DAPI-stained}} Ciliates by Flow Cytometry},
  author = {Lindstr{\"o}m, Eva S. and Stadler, Peter and Weisse, Thomas},
  year = {2003},
  journal = {Archiv f\"ur Hydrobiologie},
  volume = {157},
  number = {2},
