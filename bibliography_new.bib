@article{VanDonk2011,
  title = {Induced Defences in Marine and Freshwater Phytoplankton: A Review},
  shorttitle = {Induced Defences in Marine and Freshwater Phytoplankton},
  author = {Van Donk, Ellen and Ianora, Adrianna and Vos, Matthijs},
