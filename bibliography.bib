@article{Boria2014,
  title = {Spatial Filtering to Reduce Sampling Bias Can Improve the Performance of Ecological Niche Models},
  author = {Boria, Robert A. and Olson, Link E. and Goodman, Steven M. and Anderson, Robert P.},
  year = {2014},
  journal = {Ecological Modelling},
