@article{Ban2014,
  title = {Evidence for Multiple Stressor Interactions and Effects on Coral Reefs},
  author = {Ban, Stephen S. and Graham, Nicholas A.J. and Connolly, Sean R.},
  year = {2014},
  journal = {Global Change Biology},
  volume = {20},
