  title = {Sediment Microbial Diversity of Three Deep-Sea Hydrothermal Vents Southwest of the Azores},
  author = {Cerqueira, Teresa and Pinho, Diogo and Froufe, Hugo and Santos, Ricardo S. and Bettencourt, Raul and Egas, Concei{\c c}{\~a}o},
  year = {2017},
  journal = {Microbial Ecology},
  volume = {74},
  number = {2},
