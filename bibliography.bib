@article{Blot2011,
  title = {Light History Influences the Response of the Marine Cyanobacterium {{Synechococcus}} Sp. {{WH7803}} to Oxidative Stress},
  author = {Blot, Nicolas and {Mella-Flores}, Daniella and Six, Christophe and {le Corguill{\'e}}, Gildas and Boutte, Christophe and Peyrat, Anne and Monnier, Annabelle and Ratin, Morgane and Gourvil, Priscillia and Campbell, Douglas A. and Garczarek, Laurence},
