@article{Isomaa2013,
  title = {Baltic Cod ({{Gadus}} Morhua Callarias) Recovery Potential under Different Environment and Fishery Scenarios},
  author = {Isomaa, Marleena and Kaitala, Veijo and Laakso, Jouni},
