@article{Mouillot2008,
  title = {Do We Protect Biological Originality in Protected Areas? {{A}} New Index and an Application to the {{Bonifacio Strait Natural Reserve}}},
  author = {Mouillot, D. and Culioli, J. M. and Pelletier, D. and Tomasini, J. A.},
  year = {2008},
  journal = {Biological Conservation},
  volume = {141},
