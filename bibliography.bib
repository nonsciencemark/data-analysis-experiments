@article{Livingstone2001,
  title = {Contaminant-Stimulated Reactive Oxygen Species Production and Oxidative Damage in Aquatic Organisms},
  author = {{Livingstone}},
  year = {2001},
  journal = {Marine Pollution Bulletin},
  volume = {42},
  number = {8},
  pages = {656--666},
  issn = {0025326X},
  doi = {10.1016/S0025-326X(01)00060-1},
  abstract = {An increasing variety of industrial, agricultural and other chemicals is entering the aquatic environment and being taken up into the tissues of invertebrates and vertebrates. The uptake of these contaminants can occur from sediments, suspended particulate material, water-column and food-sources; and the major routes of input will depend on the particular dietary and ecological lifestyles of the organisms. Such a diverse array of chemicals can have many different mechanisms of toxicity, and several mechanisms may exist for a single contaminant, each contributing to various degrees to the final overall deleterious effect. Since the discovery of the importance of radical reactions in normal biological processes, and in the mechanisms of toxicity of many foreign compounds (xenobiotics), there has been an explosion of research into pro-oxidant and antioxidant processes, principally in mammalian systems. Of more recent interest has been evidence to indicate that contaminant-stimulated 'reactive oxygen species' (ROS) production and resulting oxidative damage may be a mechanism of toxicity in aquatic organisms exposed to pollution. The main aims of this short review are to briefly summarize current knowledge and recent advances in the understanding of such processes in aquatic organisms, and to assess these findings in relation to the key question of whether contaminant-stimulated ROS production and oxidative damage pose a threat to animal health and fitness in the aquatic environment.},
  isbn = {0025-326X (Print)0025-326X (Linking)},
  pmid = {11525283}
}

