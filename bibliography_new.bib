@article{Gibert2022,
  title = {Feedbacks between Size and Density Determine Rapid Eco-Phenotypic Dynamics},
  author = {Gibert, Jean P. and Han, Ze-Yi and Wieczynski, Daniel J. and Votzke, Samantha and Yammine, Andrea},
  year = {2022},
  journal = {Functional Ecology},
  volume = {36},
  number = {7},
