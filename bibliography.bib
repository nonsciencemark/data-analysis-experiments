@article{Baldwin2002,
  title = {Comparative Growth and Feeding in Zebra and Quagga Mussels ({{Dreissena}} Polymorpha and {{Dreissena}} Bugensis): Implications for {{North American}} Lakes},
  author = {Baldwin, B S and Mayer, M S and Dayton, J and Pau, N and Mendilla, J and Sullivan, M and Moore, a and Ma, a and Mills, E L},
