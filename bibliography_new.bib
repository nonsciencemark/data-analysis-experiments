@article{Johnston2019,
  title = {Predicting Population Responses to Environmental Change from Individual-Level Mechanisms: {{Towards}} a Standardized Mechanistic Approach},
  author = {Johnston, A. S.A. and Boyd, R. J. and Watson, J. W. and Paul, A. and Evans, L. C. and Gardner, E. L. and Boult, V. L.},
