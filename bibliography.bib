}

@article{Mukherjee2016,
  title = {Immunomodulatory Effects of Temperature and {{pH}} of Water in an {{Indian}} Freshwater Sponge},
  author = {Mukherjee, Soumalya and Bhunia, Anindya Sundar and Bhunia, Niladri Sekhar and Ray, Mitali and Ray, Sajal},
