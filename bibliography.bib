}

@article{Bowker2021,
  title = {Community Composition Influences Ecosystem Resistance and Production More than Species Richness or Intraspecific Diversity},
  author = {Bowker, Matthew A. and {Rengifo-Faiffer}, M. Cristina and Antoninka, Anita J. and Grover, Henry S. and Coe, Kirsten K. and Fisher, Kirsten and Mishler, Brent D. and Oliver, Mel and Stark, Lloyd R.},
