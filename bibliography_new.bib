}

@article{Flynn2013,
  title = {Misuse of the Phytoplankton{\textendash}Zooplankton Dichotomy: The Need to Assign Organisms as Mixotrophs within Plankton Functional Types},
  shorttitle = {Misuse of the Phytoplankton{\textendash}Zooplankton Dichotomy},
  author = {Flynn, Kevin J. and Stoecker, Diane K. and Mitra, Aditee and Raven, John A. and Glibert, Patricia M. and Hansen, Per Juel and Gran{\'e}li, Edna and Burkholder, Joann M.},
  year = {2013},
