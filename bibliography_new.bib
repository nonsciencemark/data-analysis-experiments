  keywords = {biolog{\'i}a de la reintroducci{\'o}n,conocimiento confiable,gesti{\'o}n,management,population restoration,recuperaci{\'o}n de especies,reintroduction biology,reliable knowledge,restauraci{\'o}n de la poblaci{\'o}n,species recovery,translocaci{\'o}n,translocation},
