@article{Lafferty2015,
  title = {A General Consumer-Resource Population Model},
  author = {Lafferty, Kevin D. and DeLeo, Giulio and Briggs, Cheryl J. and Dobson, Andrew P. and Gross, Thilo and Kuris, Armand M.},
