@article{Lester2009,
  title = {Biological Effects within No-Take Marine Reserves: {{A}} Global Synthesis},
  author = {Lester, Sarah E. and Halpern, Benjamin S. and {Grorud-Colvert}, Kirsten and Lubchenco, Jane and Ruttenberg, Benjamin I. and Gaines, Steven D. and Airam{\'e}, Satie and Warner, Robert R.},
