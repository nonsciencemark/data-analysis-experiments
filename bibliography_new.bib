@article{Hillebrand2018,
  title = {Biodiversity Change Is Uncoupled from Species Richness Trends: {{Consequences}} for Conservation and Monitoring},
  shorttitle = {Biodiversity Change Is Uncoupled from Species Richness Trends},
  author = {Hillebrand, Helmut and Blasius, Bernd and Borer, Elizabeth T. and Chase, Jonathan M. and Downing, John A. and Eriksson, Britas Klemens and Filstrup, Christopher T. and Harpole, W. Stanley and Hodapp, Dorothee and Larsen, Stefano and Lewandowska, Aleksandra M. and Seabloom, Eric W. and de Waal, Dedmer B. Van and Ryabov, Alexey B.},
