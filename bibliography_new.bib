@article{Everroad2006,
  title = {Biochemical Bases of Type {{IV}} Chromatic Adaptation in Marine {{Synechococcus}} Spp.},
  author = {Everroad, Craig and Six, Christophe and Partensky, Fr{\'e}d{\'e}ric and Thomas, Jean Claude and Holtzendorff, Julia and Wood, A. Michelle},
